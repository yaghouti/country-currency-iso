{
  "name": "country-currency-iso",
  "version": "0.1.0",
  "description": "Provides countries and currencies based on ISO names and symbols",
  "license": "MIT",
  "main": "build/index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "test": "mocha -r ts-node/register --project tsconfig.json */**/*.test.ts",
    "test:cov": "nyc --reporter=lcov --reporter=text pnpm run test "
  },
  "devDependencies": {
    "jest": "^27.5.1",
    "@types/jest": "27.0.2",
    "@types/node": "^18.6.1",
    "chai": "^4.3.4",
    "gts": "^3.1.0",
    "mocha": "^9.1.3",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yaghouti/country-currency-iso"
  },
  "keywords": [
    "country",
    "currency",
    "iso",
    "alpha2",
    "alpha-2"
  ],
  "author": {
    "name": "Majid Yaghouti",
    "email": "majid.yaghouti@gmail.com"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
